{
  "name": "kalinchernev.github.io",
  "homepage": "https://kalinchernev.github.io",
  "repository": "https://github.com/kalinchernev/kalinchernev.github.io",
  "author": "Kalin Chernev <kalin.chernev@gmail.com> (https://kalinchernev.github.io)",
  "description": "My blog with Gatsby.js",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rimraf .cache public",
    "format": "npm run format:ts && npm run format:scss",
    "format:scss": "stylelint \"src/**/*.scss\" --fix",
    "format:ts": "eslint \"src\" --ext .tsx,.ts --fix && prettier --write .",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:commit": "commitlint",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:staged": "lint-staged",
    "lint:ts": "eslint \"src\" --ext .tsx,.ts && prettier --check .",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "serve": "gatsby serve",
    "start": "npm run clean && gatsby develop",
    "test": "jest --config ./internal/testing/jest-config.ts",
    "test:update-snapshot": "jest --updateSnapshot --config ./internal/testing/jest-config.ts",
    "test:watch": "jest --watch --config ./internal/testing/jest-config.ts",
    "deploy": "gh-pages -d public -b master",
    "release": "run-s build deploy"
  },
  "bugs": {
    "url": "https://github.com/kalinchernev/kalinchernev.github.io/issues"
  },
  "license": "MIT",
  "keywords": [],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.tsx?": [
      "npm run format:ts"
    ],
    "*.(sc|sa|c)ss": [
      "npm run format:scss"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@alxshelepenok/diesel": "^1.0.1195",
