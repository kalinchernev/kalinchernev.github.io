{
  "name": "kalinchernev.github.io",
  "version": "2.0.0",
  "description": "My blog with Gatsby.js",
  "private": true,
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "deploy": "gh-pages -d public -b master",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "generate-app-icons": "sh ./scripts/generate-app-icons.sh",
    "lint:js": "eslint \"**/*.{js,jsx}\"",
    "lint": "run-p lint:*",
    "precommit": "lint-staged",
    "release": "run-s clean build deploy",
    "start": "yarn develop",
    "test": "run-p test:* -cn",
    "test:lint": "npm run lint"
  },
  "keywords": [
    "gatsby"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kalinchernev/kalinchernev.github.io.git"
  },
  "author": "Kalin Chernev <kalin.chernev@gmail.com> (https://kalinchernev.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kalinchernev/kalinchernev.github.io/issues"
  },
  "homepage": "https://kalinchernev.github.io",
  "dependencies": {
    "dateformat": "^3.0.2",
    "gatsby": "2.15.2",
    "gatsby-link": "2.2.11",
    "gatsby-plugin-catch-links": "2.1.7",
    "gatsby-plugin-google-analytics": "2.1.13",
    "gatsby-plugin-layout": "1.1.6",
    "gatsby-plugin-manifest": "2.2.13",
    "gatsby-plugin-nprogress": "2.1.5",
    "gatsby-plugin-offline": "3.0.1",
    "gatsby-plugin-react-helmet": "3.1.6",
    "gatsby-plugin-sharp": "2.2.19",
    "gatsby-plugin-typography": "2.3.6",
    "gatsby-remark-autolink-headers": "2.1.9",
    "gatsby-remark-copy-linked-files": "2.1.14",
    "gatsby-remark-images": "3.1.20",
    "gatsby-remark-prismjs": "3.3.12",
    "gatsby-remark-responsive-iframe": "2.2.11",
    "gatsby-source-filesystem": "2.1.19",
    "gatsby-transformer-remark": "2.6.20",
    "prismjs": "1.17.1",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "typography": "0.16.19",
    "typography-theme-moraga": "0.16.19"
  },
  "devDependencies": {
    "eslint": "6.3.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.1.0",
    "eslint-loader": "3.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "gatsby-cli": "2.7.42",
    "gh-pages": "2.1.1",
    "husky": "3.0.5",
    "lint-staged": "9.2.5",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "sharp-cli": "1.10.0"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
